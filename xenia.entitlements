<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow JIT compilation with MAP_JIT flag -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
    
    <!-- Enable JIT write callbacks for Hardened Runtime -->
    <key>com.apple.security.cs.jit-write-allowlist</key>
    <true/>
    
    <!-- Allow late freezing of callback allowlist for dynamic libraries -->
    <key>com.apple.security.cs.jit-write-allowlist-freeze-late</key>
    <true/>
    
    <!-- Allow unsigned executable memory (fallback option) -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    
    <!-- Disable library validation (may be needed for emulator plugins) -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- Allow loading of dynamic libraries -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>
</dict>
</plist>
